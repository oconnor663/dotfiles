--smart-case

# Reasonable solarized colors
--color-filename=yellow
--color-match=red
--color-lineno=reset

# Ignore tags files
--type-set=tags:is:tags
--notags

# Ignore JS bundles
--ignore-file=match:.bundle.js

# Ignore installed JS modules
--ignore-dir=node_modules

# Ignore vendored Go files
--ignore-dir=vendor
